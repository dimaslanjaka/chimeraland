    "minimatch": "^9.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nunjucks": "^3.2.4",
    "open": "^9.1.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sbg-utility": "^1.1.2",
    "sharp": "^0.32.1",
    "slugify": "^1.6.6",
    "through2": "^4.0.2",
